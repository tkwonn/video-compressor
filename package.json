{
  "name": "video-compressor",
  "version": "1.0.0",
  "description": "A GUI app to compress and convert video",
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:*",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "dev:tsc": "tsc -w -p tsconfig.node.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && cross-env NODE_ENV=\"development\" electron .",
    "start": "electron .",
    "release": "node ./build.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/node-notifier": "^8.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "autoprefixer": "^10.4.19",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "uuid": "^10.0.0"
  }
}
